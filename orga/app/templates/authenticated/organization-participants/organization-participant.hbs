<article>
  <Ui::PageTitle>
    <:breadcrumb>
      <Ui::Breadcrumb @links={{this.breadcrumbLinks}} />
    </:breadcrumb>

    <:title>
      {{t "common.fullname" firstName=@model.firstName lastName=@model.lastName}}
    </:title>

    <:tools>
      <Ui::LearnerHeaderInfo @isCertifiable={{@model.isCertifiable}} @certifiableAt={{@model.certifiableAt}} />
    </:tools>
  </Ui::PageTitle>
  {{outlet}}
</article>